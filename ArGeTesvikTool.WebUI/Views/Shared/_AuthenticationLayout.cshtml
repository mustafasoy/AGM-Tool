    <script src="~/frest/app-assets/fonts/LivIconsEvo/js/LivIconsEvo.defaults.js"></script>
    <script src="~/frest/app-assets/fonts/LivIconsEvo/js/LivIconsEvo.min.js"></script>
    <script src="~/frest/app-assets/vendors/js/extensions/toastr.min.js"></script>
    <!-- BEGIN Vendor JS-->
