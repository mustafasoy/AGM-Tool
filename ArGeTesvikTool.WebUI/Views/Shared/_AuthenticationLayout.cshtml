    <!-- BEGIN: Vendor CSS-->
    <link rel="stylesheet" type="text/css" href="~/frest/app-assets/vendors/css/vendors.min.css">
    <link rel="stylesheet" type="text/css" href="~/frest/app-assets/vendors/css/forms/select/select2.min.css">
    <link rel="stylesheet" type="text/css" href="~/frest/app-assets/vendors/css/extensions/toastr.css">
    <!-- END: Vendor CSS-->
    <!-- BEGIN: Theme CSS-->
    <link rel="stylesheet" type="text/css" href="~/frest/app-assets/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="~/frest/app-assets/css/bootstrap-extended.css">
